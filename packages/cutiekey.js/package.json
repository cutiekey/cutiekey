{
  "name": "cutiekey.js",
  "version": "0.1.0",
  "description": "The JavaScript SDK for Cutiekey",
  "license": "MIT",
  "repository": {
    "directory": "packages/cutiekey.js",
    "type": "git",
    "url": "https://activitypub.software/cutiekey/cutiekey.git"
  },
  "files": [
    "./built",
    "./LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./built/esm/index.js",
      "types": "./built/dts/index.d.ts"
    },
    "./*": {
      "import": "./built/esm/*",
      "types": "./built/dts/*"
    }
  },
  "types": "./built/dts/index.d.ts",
  "scripts": {
    "api": "api-extractor run --local --verbose",
    "api:prod": "api-extractor run --verbose",
    "build": "yarn build:esm && yarn build:dts",
    "build:dts": "tsc --declaration true --declarationMap true --emitDeclarationOnly true --outDir \"./built/dts\" -p \"./tsconfig.json\" && tsc-alias --outDir \"./built/dts\" -p \"./tsconfig.json\"",
    "build:esm": "tsc --outDir \"./built/esm\" -p \"./tsconfig.json\" && tsc-alias --outDir \"./built/esm\" -p \"./tsconfig.json\"",
    "lint": "yarn lint:types && yarn lint:eslint",
    "lint:eslint": "eslint --ext \".js,.jsx,.ts,.tsx\" \"./src/**/*\"",
    "lint:types": "tsc --noEmit",
    "test": "yarn test:jest && yarn exec tsd",
    "test:jest": "jest --coverage --detectOpenHandles",
    "update-autogen-code": "yarn workspace api-generator generate && ncp \"../api-generator/built/autogen\" \"./src/autogen\"",
    "watch": "nodemon -e \"cjs,js,json,mjs,ts\" --exec \"yarn build\" -w \"./src\""
  },
  "dependencies": {
    "eventemitter3": "^5.0.1",
    "reconnecting-websocket": "^4.4.0"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.40.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-websocket-mock": "^2.5.0",
    "mock-socket": "^9.3.1",
    "ncp": "^2.0.0",
    "nodemon": "^3.0.3",
    "tsc-alias": "^1.8.8",
    "tsd": "^0.30.4",
    "typescript": "^5.3.3"
  }
}
