{
  "name": "sw",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "node ./build.js",
    "build:prod": "cross-env NODE_ENV=production node ./build.js",
    "lint": "yarn lint:eslint && yarn lint:types",
    "lint:eslint": "eslint --ext \".js,.jsx,.ts,.tsx\" \"./src/**/*\"",
    "lint:types": "tsc --noEmit",
    "watch": "nodemon -e json --exec \"node ./build.js watch\" -w \"../../package.json\""
  },
  "dependencies": {
    "cutiekey.js": "workspace:*",
    "idb-keyval": "^6.2.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@typescript/lib-webworker": "npm:@types/serviceworker@^0.0.83",
    "cross-env": "^7.0.3",
    "esbuild": "^0.20.0",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "nodemon": "^3.0.3",
    "typescript": "^5.3.3"
  }
}
